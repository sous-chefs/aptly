# Generated by chef for node: <%= node["hostname"] %>

server {

    listen 80;

    root <%= node["aptly"]["public"]["dir"] %>;
    server_name <%= node["aptly"]["public"]["fqdn"] %>;
  
    access_log /var/log/nginx/aptly_access.log;
    error_log /var/log/nginx/aptly_error.log;

    location / {
        autoindex on;
    }
}
